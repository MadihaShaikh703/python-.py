print("251A018 , 17/2/26")
try:
    val1 = int(input("Enter first value :"))
    val2 = int(input("Enter second value :"))
    result = val1 / val2
except ZeroDivisionError:
    print("Number cannot be divided by zero.")
except ValueError:
    print("Invalid Input")
else:
    print("Division :",result)
finally:
    print("Thankyou.")
